<div>
  <% if flash[:notice] %>
    <p class="three" role="alert"><%= notice %></p>
  <% end %>
  <div class="four">
    <h1>recipes</h1>
    <div>
      <%= link_to "add recipe", new_recipe_path, class: "" %>
    </div>
  </div>
  <div id="five">
    <table class="">
      <%= render partial: "recipe_header"  %>
      <tbody>
        <% @recipes.each do |recipe| %>
          <tr>
            <%= render recipe %>
            <td><%= link_to "display", recipe_path(recipe.id), class: "" %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>